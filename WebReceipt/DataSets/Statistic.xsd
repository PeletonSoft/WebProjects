                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT TOP 25
	SI.[Код артикула],
	S.[Артикул],
	S.[Описание],
	SD.[Аббревиатура] AS [Склад],
	SUM(SIM.[Количество]) AS [Количество]
FROM [Склад] AS S
	INNER JOIN [Склад_информация] AS SI
		ON SI.[Код артикула]=S.[Код]
	INNER JOIN [Склад_информация_размещение] AS SIM
		ON SIM.[Код информации]=SI.[Код]
	INNER JOIN [Признаки_информации] AS A
		ON A.[Код]=SI.[Признак информации]
	INNER JOIN [Склады_отделы] AS SD
		ON SD.[Код]=SIM.[Код отдела склада]
	INNER JOIN [Виды_товаров] AS TP
		ON TP.[Код]=S.[Код вида товара]
	INNER JOIN [Производители] AS PR
		ON PR.[Код]=S.[Код производителя]
	INNER JOIN [Склады_отделы_доступ] AS SDA
		ON
			SDA.[Код отдела склада]=SIM.[Код отдела склада] AND
			SDA.[Код магазина]= @ShopId
WHERE 
	S.[Артикул]+' '+ISNULL(S.[Описание],'') LIKE '%'+@Filter +'%'
GROUP BY
	SI.[Код артикула],
	S.[Артикул],
	S.[Описание],
	SD.[Аббревиатура]
ORDER BY 
	SI.[Код артикула] ASC,
	SD.[Аббревиатура] ASC </CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="ShopId" ColumnName="Код магазина" DataSourceName="Магазин00.dbo.Склады_отделы_доступ" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@ShopId" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="Код магазина" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="false" AutogeneratedName="Filter" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="String" Direction="Input" ParameterName="@Filter" Precision="0" Scale="0" Size="1024" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
