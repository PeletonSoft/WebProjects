          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="taSearchArticle" GeneratorDataComponentClassName="taSearchArticle" Name="tbSearchArticle" UserDataComponentName="taSearchArticle">
